<h1>Welcome to SoftUni Wiki!</h1>
<section>
	<h2>Search results for: "{{filters}}"</h2>
	<div class="spacer">
		<ul>
			{{#each articles}}
			<li>
				<a href="/article/details/{{_id}}`">{{title}}</a>
			</li>
			
			{{else}}
			<p>No articles for searched criteria</p>
			{{/each}}
		</ul>
	</div>
</section>