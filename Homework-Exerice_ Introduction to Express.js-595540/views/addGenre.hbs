<h1>Add New Movie</h1>
<div class="container">
  <form action="/addGenre" enctype="multipart/form-data" method="POST">
    <label for="movieTitle">Title</label>
    <input type="text" id="movieTitle" name="genreName" placeholder="The title of the movie">
    <input value="Submit" type="submit">
  </form>
</div>
{{#if status}}
<div id="succssesBox">
  <h2 id="seccssesMsg">Genre added</h2>
</div>
{{/if}}